{
  "name": "zingmp3",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "-": "^0.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "D": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "stylelint-scss": "^3.20.1",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint-staged:prettier": "prettier --fix \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint './src/**/*.{js}'",
    "lint-staged:js": "eslint './src/**/*.{js,jsx}' --fix",
    "lint-staged:style": "stylelint './src/**/*.scss' --fix",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/node-logger": "^6.3.6",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.6",
    "antd": "^4.16.10",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "4",
    "lint-staged": "^11.1.1",
    "node": "^16.6.1",
    "prettier": "^2.2.1",
    "sass": "^1.37.5",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js, jsx, scss, md, json}": [
      "yarn lint-staged:prettier",
      "git add"
    ],
    "src/**/*.{js, jsx}": [
      "yarn lint-staged:js",
      "git add"
    ],
    "src/**/*.{scss}": [
      "yarn lint-staged:style",
      "git add"
    ]
  }
}
